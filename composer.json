        "wiki": "https://github.com/ruwmapps/dynamicform/wiki/",
        "source": "https://github.com/ruwmapps/dynamicform"
    },
    "authors": [
        {
            "name": "Web Mobille Apps",
            "email": "dev@wmapps.ru",
            "homepage": "http://wmapps.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.5",
         "symfony/css-selector": "^6.4|^7.0",
